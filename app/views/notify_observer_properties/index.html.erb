<div class="boxforforms">
<h3>Listing notify observer properties</h3>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notify_observer_properties.each do |notify_observer_property| %>
  <tr>
    <td><%= notify_observer_property.name %></td>
    <td><%= link_to 'Show', notify_observer_property, :class => "btn btn-info" %></td>
    <td><%= link_to 'Edit', edit_notify_observer_property_path(notify_observer_property), :class => "btn btn-info" %></td>
    <td><%= link_to 'Destroy', notify_observer_property, confirm: 'Are you sure?', method: :delete, :class => "btn btn-danger" %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Notify observer property', new_notify_observer_property_path, :class =>"btn btn-primary" %>
</div>
