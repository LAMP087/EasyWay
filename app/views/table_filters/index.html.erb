<div class="boxforforms">
<h3>Listing table filters</h3>
<table id="ResourceTable" class="table table-striped">
  <tr>
    <th>Table template</th>
    <th>Resource</th>
    <th>Position num</th>
    <th>Position float</th>
    <th>Delimiter</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @table_filters.each do |table_filter| %>
  <tr>
    <td><%= table_filter.table_template_id %></td>
    <td><%= table_filter.resource_id %></td>
    <td><%= table_filter.position_num %></td>
    <td><%= table_filter.position_float %></td>
    <td><%= table_filter.delimiter %></td>
    <td><%= link_to 'Show', table_filter, :class => "btn btn-info" %></td>
    <td><%= link_to 'Edit', edit_table_filter_path(table_filter), :class => "btn btn-success" %></td>
    <td><%= link_to 'Destroy', table_filter, method: :delete, :class => "btn btn-danger" ,data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
</div>