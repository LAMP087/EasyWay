<div class="boxforforms">
<%= form_for @notify_observer, :html =>{:class=>"form-horizontal"} do |f| %>
  <% if @notify_observer.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@notify_observer.errors.count, "error") %> prohibited this notify_observer from being saved:</h2>

      <ul>
      <% @notify_observer.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div id="notify_observer_fields">
    <div class="control-group" >
        <%= f.label :name, :class=>"control-label" %>
        <div class="controls controls-no">
          <%= f.text_field :name %>
        </div>
    </div>
  </div>


    
  <div class="control-group">
    <div class="controls">
    <%= link_to "Add notify_observer_property","#", :onclick => "notify_observer_add_new_field(this)", :class =>"btn btn-primary" %>
	</div>
  </div>
  	
  <div class="actions">
    <%= f.submit :class =>"btn btn-primary"%>
  </div>
<% end %>
</div>
