<div class="boxforforms">
<h3>Listing notify events</h3>

<table class="table table-striped">
  <tr>
    <th>Template</th>
    <th>Event</th>
    <th>Name</th>
    <th>Observer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notify_events.each do |notify_event| %>
  <tr>
    <td><%= notify_event.notify_template_id %></td>
    <td><%= notify_event.event_id %></td>
    <td><%= notify_event.name %></td>
    <td><%= notify_event.notify_observer_id %></td>
    <td><%= link_to 'Show', notify_event, :class => "btn btn-info" %></td>
    <td><%= link_to 'Edit', edit_notify_event_path(notify_event), :class => "btn btn-info" %></td>
    <td><%= link_to 'Destroy', notify_event, confirm: 'Are you sure?', method: :delete, :class => "btn btn-danger" %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Notify Event', new_notify_event_path, :class => "btn btn-primary" %>
</div>
